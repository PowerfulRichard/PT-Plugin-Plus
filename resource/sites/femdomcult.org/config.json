{
  "name": "Femdomcult",
  "timezoneOffset": "-1100",
  "description": "成人",
  "url": "https://femdomcult.org/",
  "icon": "https://femdomcult.org/favicon.ico",
  "tags": [
    "成人"
  ],
  "schema": "FDC",
  "host": "femdomcult.org",
  "searchEntry": [{
    "entry": "/torrents.php?searchtext=$key$",
    "name": "all",
    "resultType": "html",
    "parseScriptFile": "/schemas/Gazelle/getSearchResult.js",
    "resultSelector": "table.torrent_table:last > tbody > tr",
    "enabled": true
  }],
  "selectors": {
    "userBaseInfo": {
      "page": "/index.php",
      "fields": {
        "id": {
          "selector": ["#nav_userinfo a.username"],
          "attribute": "href",
          "filters": ["query.text().match(/id=.+(\\d+)/)"]        
        },
        "name": {
          "selector": ["a.username"]
        },
        "bonus": {
          "selector": ["span#stats_credits"]
        },
        "ratio": {
          "selector": ["span.r20"]
        }
      }
    },
    "userExtendInfo": {
      "page": "/user.php?id=$user.id$",
      "fields": {
        "uploaded": {
          "selector": "div:contains('Stats') + ul.stats > li:contains('Uploaded')",
          "filters": ["query.text().replace(/,/g,'').match(/Uploaded:.+?([\\d.]+ ?[ZEPTGMK]?i?B)/)", "(query && query.length>=2)?(query[1]).sizeToNumber():0"]
        },
        "downloaded": {
          "selector": "div:contains('Stats') + ul.stats > li:contains('Downloaded')",
          "filters": ["query.text().replace(/,/g,'').match(/Downloaded:.+?([\\d.]+ ?[ZEPTGMK]?i?B)/)", "(query && query.length>=2)?(query[1]).sizeToNumber():0"]
        },
        "levelName": {
          "selector": "div:contains('Personal') + ul.stats > li:contains('Class:')",
          "filters": ["query.text().match(/Class:.+?(.+)/)", "(query && query.length>=2)?query[1]:''"]
        },
        "joinTime": {
          "selector": ["div.sidebar>div:nth-child(6)>ul>li:nth-child(1)>span"]
        },
        "uploads": {
          "selector": ["div.sidebar>div:nth-child(12)>ul>li:nth-child(9)"]
        }
      }
    }
  },
  "levelRequirements": [{
    "level": "1", 
    "name": "Beginner",
    "interval": "1",
    "ratio": "0",
    "privilege": "Can upload, can vote on requests but is limited to downloading a total of 10GB per 24 hours (includes freeleech)."
  },{
    "level": "2", 
    "name": "Perv",
    "interval": "1",
    "uploaded": "10GB",
    "ratio": "0.7",
    "privilege": "Can make content requests in Requests area, can access Top 10, can use the notifications system, can bookmark torrents, can create collages and can create a personal collage."
  },{
    "level": "3", 
    "name": "Member",
    "interval": "4",
    "uploaded": "25GB",
    "ratio": "1.05",
    "uploads": "10",
    "privilege": "Can view site statistics, can create polls in the forums, can add multiple tags at once, can access the invite forums, can set and can use a torrent signature."
  },{
    "level": "4", 
    "name": "Senior Member",
    "interval": "4",
    "uploaded": "1TB",
    "ratio": "1.05",
    "uploads": "50",
    "privilege": "Can use advanced bbcode tags and can download multiple torrents at once."
  },{
    "level": "5", 
    "name": "Uploader",
    "interval": "8",
    "uploaded": "5TB",
    "ratio": "1.05",
    "uploads": "150",
    "privilege": "Can view torrent snatchlists, can view torrent peerlists, can delete tags, can make freeleech torrents, can download VIP torrents and can use invites when necessary."
  }
  ],
  "collaborator": [
    "RichardHu"
  ],
  "supportedFeatures": {
    "imdbSearch": false,
    "userData": "◐"
  }
}
